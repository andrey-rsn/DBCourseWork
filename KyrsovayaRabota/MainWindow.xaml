<Window x:Class="KyrsovayaRabota.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KyrsovayaRabota"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="500">
    <Grid>
        <Label x:Name="Label1" Content="Проверочноый расчет передачи зубчатым ремнем на&#xD;&#xA;выполнение условий сохранения зацепления в передаче" HorizontalAlignment="Center" Height="77" Margin="0,10,0,0" VerticalAlignment="Top" Width="438" FontFamily="Arial" FontSize="16"/>
        <Button x:Name="GoToUzButton" Content="Начать новый расчёт" HorizontalAlignment="Center" Margin="0,111,0,0" VerticalAlignment="Top" Height="36" Width="160" Click="GoToUzButton_Click" RenderTransformOrigin="0.501,0.599"/>
        <Button x:Name="ExitButton" Content="Выход" HorizontalAlignment="Center" Height="34" Margin="0,152,0,0" VerticalAlignment="Top" Width="161" Click="ExitButton_Click"/>

    </Grid>
</Window>
